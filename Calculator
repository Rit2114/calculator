<script>

function inputData(data){
	document.getElementById("ress").value += data;
}

function clearData(){
	document.getElementById("ress").value="";
}
function calculate(){
 var data=document.getElementById("ress").value;
var data1 = eval(data)
value=document.getElementById("ress").value= data1;
}

function clearLast(){
var d = document.getElementById("ress").value;
document.getElementById("ress").value = d.substring(0, d.length-1);
}
</script>
